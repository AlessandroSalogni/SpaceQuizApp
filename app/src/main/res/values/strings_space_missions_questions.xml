    <!-- Space Missions questions -->
    <string name="space_missions_question_number"><xliff:g>8</xliff:g></string>

    <string name="space_missions_question_1_text"><xliff:g>[Question]</xliff:g>What is the name of the first human-made object to orbit Earth?</string>
    <string name="space_missions_question_1_answer_1"><xliff:g>[Answer]</xliff:g>Apollo 1</string>
    <string name="space_missions_question_1_answer_2"><xliff:g>[Answer]</xliff:g>Opportunity</string>
    <string name="space_missions_question_1_answer_3"><xliff:g>[Answer][Correct]</xliff:g>Sputnik 1</string>
    <string name="space_missions_question_1_answer_4"><xliff:g>[Answer]</xliff:g>Spaceway 1</string>

    <string name="space_missions_question_2_text"><xliff:g>[Question]</xliff:g>In what year did the first Moon Landing?</string>
    <string name="space_missions_question_2_correct_answer"><xliff:g>[Answer][Correct]</xliff:g>1969</string>

    <string name="space_missions_question_3_text"><xliff:g>[Question]</xliff:g>Who composed the equipage of Apollo 11?</string>
    <string name="space_missions_question_3_answer_1"><xliff:g>[Answer][Correct]</xliff:g>Michael Collins</string>
    <string name="space_missions_question_3_answer_2"><xliff:g>[Answer][Correct]</xliff:g>Neil Armstrong</string>
    <string name="space_missions_question_3_answer_3"><xliff:g>[Answer][Correct]</xliff:g>Buzz Aldrin</string>
    <string name="space_missions_question_3_answer_4"><xliff:g>[Answer]</xliff:g>Glynn Lunney</string>

    <string name="space_missions_question_4_text"><xliff:g>[Question]</xliff:g>Who was the first man in the space?</string>
    <string name="space_missions_question_4_answer_1"><xliff:g>[Answer]</xliff:g>Valentina Tereshkova</string>
    <string name="space_missions_question_4_answer_2"><xliff:g>[Answer]</xliff:g>Neil Armstrong</string>
    <string name="space_missions_question_4_answer_3"><xliff:g>[Answer]</xliff:g>John Glenn Jr.</string>
    <string name="space_missions_question_4_answer_4"><xliff:g>[Answer][Correct]</xliff:g>Yuri Gagarin</string>

    <string name="space_missions_question_5_text"><xliff:g>[Question]</xliff:g>Cape Canaveral is the seat of the Kennedy Space Center?</string>
    <string name="space_missions_question_5_answer_1"><xliff:g>[Answer][Correct]</xliff:g>true</string>
    <string name="space_missions_question_5_answer_2"><xliff:g>[Answer]</xliff:g>false</string>

    <string name="space_missions_question_6_text"><xliff:g>[Question]</xliff:g>Which of these are rovers sent to Mars?</string>
    <string name="space_missions_question_6_answer_1"><xliff:g>[Answer][Correct]</xliff:g>Opportunity</string>
    <string name="space_missions_question_6_answer_2"><xliff:g>[Answer]</xliff:g>Lunokhod 1</string>
    <string name="space_missions_question_6_answer_3"><xliff:g>[Answer]</xliff:g>Yutu</string>
    <string name="space_missions_question_6_answer_4"><xliff:g>[Answer][Correct]</xliff:g>Spirit</string>

    <string name="space_missions_question_7_text"><xliff:g>[Question]</xliff:g>Which planet was to study the Cassini mission?</string>
    <string name="space_missions_question_7_answer_1"><xliff:g>[Answer][Correct]</xliff:g>Saturn</string>
    <string name="space_missions_question_7_answer_2"><xliff:g>[Answer]</xliff:g>Neptune</string>
    <string name="space_missions_question_7_answer_3"><xliff:g>[Answer]</xliff:g>Jupiter</string>
    <string name="space_missions_question_7_answer_4"><xliff:g>[Answer]</xliff:g>Mercury</string>

    <string name="space_missions_question_8_text"><xliff:g>[Question]</xliff:g>Did the first component of the International Space Station launched into orbit in 1998?</string>
    <string name="space_missions_question_8_answer_1"><xliff:g>[Answer][Correct]</xliff:g>True</string>
    <string name="space_missions_question_8_answer_2"><xliff:g>[Answer]</xliff:g>False</string>
